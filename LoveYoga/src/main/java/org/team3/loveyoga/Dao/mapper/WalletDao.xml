  <insert id="insert" parameterType="org.team3.loveyoga.pojo.Wallet">
    insert into y_wallet (yw_id, yw_cardid, yw_balance, 
      yw_flag)
    values (#{ywId,jdbcType=INTEGER}, #{ywCardid,jdbcType=INTEGER}, #{ywBalance,jdbcType=DECIMAL}, 
      #{ywFlag,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="org.team3.loveyoga.pojo.Wallet">
    insert into y_wallet
    <trim prefix="(" suffix=")" suffixOverrides=",">
