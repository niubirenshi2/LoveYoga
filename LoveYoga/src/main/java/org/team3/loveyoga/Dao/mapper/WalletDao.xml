        #{ywId,jdbcType=INTEGER},
      </if>
      <if test="ywCardid != null">
        #{ywCardid,jdbcType=INTEGER},
      </if>
      <if test="ywBalance != null">
        #{ywBalance,jdbcType=DECIMAL},
      </if>
      <if test="ywFlag != null">
        #{ywFlag,jdbcType=INTEGER},
      </if>
