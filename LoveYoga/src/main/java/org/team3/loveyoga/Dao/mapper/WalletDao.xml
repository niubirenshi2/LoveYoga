        yw_id,
      </if>
      <if test="ywCardid != null">
        yw_cardid,
      </if>
      <if test="ywBalance != null">
        yw_balance,
      </if>
      <if test="ywFlag != null">
        yw_flag,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
