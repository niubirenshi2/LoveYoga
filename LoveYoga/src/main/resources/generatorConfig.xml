<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="mysql" targetRuntime="MyBatis3">
  	<commentGenerator>
  		<property name="suppressDate" value="true"/>
  		<property name="suppressAllComments" value="true"/>
  	</commentGenerator>
  
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/loveyoga?useUnicode=true&amp;characterEncoding=utf-8"
        userId="root"
        password="123456">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!-- 生成实体类的路径 -->
    <javaModelGenerator targetPackage="org.team3.loveyoga.pojo" targetProject="LoveYoga"></javaModelGenerator>
	
	<!-- 生成sql映射文件的路径 -->
    <sqlMapGenerator targetPackage="org.team3.loveyoga.Dao.mapper"  targetProject="LoveYoga"></sqlMapGenerator>

	<!-- 生产dao接口的目录 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="org.team3.loveyoga.Dao"  targetProject="LoveYoga"></javaClientGenerator>

	<!-- 配置表 -->
	<!-- 教练登录表 -->
    <table tableName="y_user" mapperName="CoachDao" domainObjectName="Coach"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
	<!-- 教练信息表 -->
    <table tableName="y_coach" mapperName="CoachInfoDao" domainObjectName="CoachInfo"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
    
    <!-- 头像表 -->
    <table tableName="y_headImg" mapperName="HeadImgDao" domainObjectName="HeadImg"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
     <!-- 昵称表 -->
    <table tableName="y_nickname" mapperName="NicknameDao" domainObjectName="Nickname"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>

    <!-- 我的钱包表 -->
    <table tableName="y_wallet" mapperName="WalletDao" domainObjectName="Wallet"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
        <!-- 银行卡表 -->
    <table tableName="y_bankcard_choose" mapperName="Bankcard_chooseDao" domainObjectName="Bankcard_choose"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
        <!-- 签约场馆表 -->
    <table tableName="y_sign_gym" mapperName="Sign_gymDao" domainObjectName="Sign_gym"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
        <!-- 教练取钱记录表 -->
    <table tableName="y_consume_log" mapperName="Consume_logDao" domainObjectName="Consume_logImg"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
   
        <!-- 角色表 -->
    <table tableName="y_role" mapperName="RoleDao" domainObjectName="Role"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
        <!-- 用户角色表 -->
    <table tableName="y_ur" mapperName="UserRoleDao" domainObjectName="UserRole"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
        <!-- 用户角色表 -->
    <table tableName="y_ur" mapperName="UserRoleDao" domainObjectName="UserRole"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
     <!-- 角色权限表 -->
    <table tableName="y_rp" mapperName="RoleAccessDao" domainObjectName="RoleAccess"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
    <!-- 权限表 -->
    <table tableName="y_perm" mapperName="PermissionDao" domainObjectName="Permission"
    enableDeleteByExample="false"
    enableUpdateByExample="false"
    enableSelectByExample="false"
    enableCountByExample="false"
    selectByExampleQueryId="false" ></table>
    
  </context>
</generatorConfiguration>